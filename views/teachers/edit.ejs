<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>Ini Adalah Halaman Edit Teacher</h1>
    <% if (dataError) { %>
      <p><%= dataError %></p>
    <% }%>
  </body>
  <form class="" action="/teachers/edit/<%= dataTeacher.id %>" method="post">
    <input type="text" name="first_name" value="<%= dataTeacher.first_name %>" required>
    <input type="text" name="last_name" value="<%= dataTeacher.last_name %>" required>
    <input type="text" name="email" value="<%= dataTeacher.email %>">
    <input type="submit" value="SUBMIT">
  </form>
</html>
